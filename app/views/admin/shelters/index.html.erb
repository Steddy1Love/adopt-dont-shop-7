<h1> Shelters </h1>
    <div id="shelters_list">
        <% @admin_shelters.each do |shelter| %>
            <p> <%= shelter.name %> </p>
        <% end %>
    </div>
<h2> Shelters with Pending Applications </h2>
    <div id="pending_app_shelters_list">
        <% @pending_app_shelters.each do |shelter| %>
            <p> <%= shelter.name %> </p>
        <% end %>
    </div>